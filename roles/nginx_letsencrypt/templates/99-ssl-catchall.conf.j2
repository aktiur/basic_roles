# {{ ansible_managed }}

server {
  listen 443 ssl http2 default_server;

  ssl_certificate     /etc/letsencrypt/live/{{ letsencrypt_main_domain }}/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/{{ letsencrypt_main_domain }}/privkey.pem;


  server_name _;

  location / {
    deny all;
  }

  include /etc/nginx/letsencrypt_acme_params;
}
